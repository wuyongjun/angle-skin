<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="Bootstrap Admin App + jQuery">
    <meta name="keywords" content="app, responsive, jquery, bootstrap, dashboard, admin">
    <title>@ViewData["Title"] - Bootstrap Admin Template</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- Place styles for plugins -->
    @if (IsSectionDefined("Styles"))
    {@RenderSection("Styles", required: false)}

    <environment names="Development">
        <!-- =============== VENDOR STYLES ===============-->
        <link href="~/lib/fontawesome/css/font-awesome.css" rel="stylesheet" />
        <link href="~/lib/simple-line-icons/css/simple-line-icons.css" rel="stylesheet" />
        <!-- =============== BOOTSTRAP STYLES ===============-->
        <link href="~/css/bootstrap.css" rel="stylesheet" />
        <!-- =============== APP STYLES ===============-->
        <link href="~/css/app.css" rel="stylesheet" asp-append-version="true" />
    </environment>
    <environment names="Staging,Production">
        <!-- =============== VENDOR STYLES ===============-->
        <link href="~/lib/fontawesome/css/font-awesome.min.css" rel="stylesheet" />
        <link href="~/lib/simple-line-icons/css/simple-line-icons.css" rel="stylesheet" />
        <!-- =============== BOOTSTRAP STYLES ===============-->
        <link href="~/css/bootstrap.css" rel="stylesheet" />
        <!-- =============== APP STYLES ===============-->
        <link href="~/css/app.css" rel="stylesheet" asp-append-version="true" />
    </environment>


</head>
<body>
    <div class="wrapper">
        @RenderBody()
    </div>
    <div class="p-lg text-center">
        <span>&copy;</span>
        <span>2016</span>
        <span>-</span>
        <span>Angle</span>
        <br>
        <span>Bootstrap Admin Template</span>
    </div>

    <environment names="Development">

        <!-- =============== VENDOR SCRIPTS ===============-->

        <script src="~/lib/modernizr/modernizr.custom.js"></script>
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/lib/jQuery-Storage-API/jquery.storageapi.js"></script>
        <script src="~/lib/parsleyjs/dist/parsley.js"></script>

        <!-- =============== PAGE VENDOR SCRIPTS ===============-->
        @RenderSection("scripts", required: false)

        <!-- =============== APP SCRIPTS ===============-->
        <!-- App init -->
        <script src="~/master/js/app.init.js"></script>
        <!-- Modules -->
        <script src="~/master/js/modules/bootstrap-start.js"></script>
        <script src="~/master/js/modules/calendar.js"></script>
        <script src="~/master/js/modules/classyloader.js"></script>
        <script src="~/master/js/modules/clear-storage.js"></script>
        <script src="~/master/js/modules/constants.js"></script>
        <script src="~/master/js/modules/flatdoc.js"></script>
        <script src="~/master/js/modules/fullscreen.js"></script>
        <script src="~/master/js/modules/gmap.js"></script>
        <script src="~/master/js/modules/load-css.js"></script>
        <script src="~/master/js/modules/localize.js"></script>
        <script src="~/master/js/modules/maps-vector.js"></script>
        <script src="~/master/js/modules/navbar-search.js"></script>
        <script src="~/master/js/modules/notify.js"></script>
        <script src="~/master/js/modules/now.js"></script>
        <script src="~/master/js/modules/panel-tools.js"></script>
        <script src="~/master/js/modules/play-animation.js"></script>
        <script src="~/master/js/modules/porlets.js"></script>
        <script src="~/master/js/modules/sidebar.js"></script>
        <script src="~/master/js/modules/skycons.js"></script>
        <script src="~/master/js/modules/slimscroll.js"></script>
        <script src="~/master/js/modules/sparkline.js"></script>
        <script src="~/master/js/modules/table-checkall.js"></script>
        <script src="~/master/js/modules/toggle-state.js"></script>
        <script src="~/master/js/modules/utils.js"></script>
        <script src="~/master/js/modules/chart.js"></script>
        <script src="~/master/js/modules/morris.js"></script>
        <script src="~/master/js/modules/rickshaw.js"></script>
        <script src="~/master/js/modules/chartist.js"></script>
        <script src="~/master/js/modules/tour.js"></script>
        <script src="~/master/js/modules/sweetalert.js"></script>
        <script src="~/master/js/modules/color-picker.js"></script>
        <script src="~/master/js/modules/imagecrop.js"></script>
        <script src="~/master/js/modules/chart-knob.js"></script>
        <script src="~/master/js/modules/chart-easypie.js"></script>
        <script src="~/master/js/modules/select2.js"></script>
        <!-- Demos -->
        <script src="~/master/js/modules/demo/demo-datatable.js"></script>
        <script src="~/master/js/modules/demo/demo-flot.js"></script>
        <script src="~/master/js/modules/demo/demo-forms.js"></script>
        <script src="~/master/js/modules/demo/demo-panels.js"></script>
        <script src="~/master/js/modules/demo/demo-search.js"></script>
        <script src="~/master/js/modules/demo/demo-vector-map.js"></script>
        <script src="~/master/js/modules/demo/demo-jqgrid.js"></script>
        <script src="~/master/js/modules/demo/demo-sortable.js"></script>
        <script src="~/master/js/modules/demo/demo-nestable.js"></script>
        <script src="~/master/js/modules/demo/demo-upload.js"></script>
        <script src="~/master/js/modules/demo/demo-wizard.js"></script>
        <script src="~/master/js/modules/demo/demo-xeditable.js"></script>
        <script src="~/master/js/modules/demo/demo-jqcloud.js"></script>
        <script src="~/master/js/modules/demo/demo-rtl.js"></script>
        <!-- Custom -->
        <script src="~/master/js/custom/custom.js"></script>

    </environment>

    <environment names="Staging,Production">

        <!-- =============== VENDOR SCRIPTS ===============-->

        <script src="~/lib/modernizr/modernizr.custom.js"></script>
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
        <script src="~/lib/jQuery-Storage-API/jquery.storageapi.min.js"></script>
        <script src="~/lib/parsleyjs/dist/parsley.js"></script>

        <!-- =============== PAGE VENDOR SCRIPTS ===============-->
        @RenderSection("scripts", required: false)

        <!-- =============== APP SCRIPTS ===============-->

        <script src="~/js/app.js"></script>

    </environment>

</body>

</html>